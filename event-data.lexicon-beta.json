{
  "lexicon": 1,
  "id": "blue.deniz.atproto.event",
  "description": "Event Data Lexicon",
  "defs": {
    "EventData": {
      "type": "object",
      "properties": {
        "v": {
          "type": "integer",
          "description": "The version of the Event Data schema"
        },
        "id": {
          "type": "string"
        },
        "name": {
          "type": "ref",
          "ref": "Translations",
          "description": "The name of the event"
        },
        "description": {
          "type": "ref",
          "ref": "Translations",
          "description": "A short description of the event"
        },
        "status": {
          "type": "ref",
          "ref": "EventStatus",
          "description": "The status of the event"
        },
        "venues": {
          "type": "array",
          "items": {
            "type": "ref",
            "ref": "Venue"
          },
          "description": "The venues associated with this event"
        },
        "instances": {
          "type": "array",
          "items": {
            "type": "ref",
            "ref": "EventInstance"
          },
          "description": "The instances of the event"
        },
        "components": {
          "type": "array",
          "items": {
            "type": "ref",
            "ref": "EventComponent"
          },
          "description": "Additional components of the event"
        }
      },
      "required": [
        "v",
        "name",
        "venues",
        "instances"
      ],
      "description": "An event"
    },
    "Translations": {
      "type": "object",
      "properties": {},
      "description": "A multilingual string"
    },
    "EventStatus": {
      "type": "unknown"
    },
    "Venue": {
      "type": "union",
      "refs": [
        "PhysicalVenue",
        "OnlineVenue"
      ]
    },
    "PhysicalVenue": {
      "type": "object",
      "properties": {
        "venueType": {
          "type": "string"
        },
        "venueId": {
          "type": "string",
          "description": "ID of the venue to be used in Event Instances"
        },
        "venueName": {
          "type": "ref",
          "ref": "Translations",
          "description": "The name of the venue"
        },
        "address": {
          "type": "ref",
          "ref": "Address"
        },
        "coordinates": {
          "type": "ref",
          "ref": "LatLng",
          "description": "Approximate coordinates"
        },
        "googleMapsPlaceId": {
          "type": "string"
        }
      },
      "required": [
        "venueType",
        "venueId",
        "venueName"
      ],
      "description": "A venue with a known or unknown physical location"
    },
    "Address": {
      "type": "object",
      "properties": {
        "countryCode": {
          "type": "string",
          "description": "The ISO 3166-1 alpha-2 country code"
        },
        "postalCode": {
          "type": "string",
          "description": "The postal code of the address, if any"
        },
        "addr": {
          "type": "string",
          "description": "Full address excluding country and postal code"
        }
      }
    },
    "LatLng": {
      "type": "object",
      "properties": {
        "lat": {
          "type": "integer"
        },
        "lng": {
          "type": "integer"
        }
      },
      "required": [
        "lat",
        "lng"
      ]
    },
    "OnlineVenue": {
      "type": "object",
      "properties": {
        "venueType": {
          "type": "string"
        },
        "venueId": {
          "type": "string",
          "description": "ID of the venue to be used in Event Instances"
        },
        "venueName": {
          "type": "ref",
          "ref": "Translations",
          "description": "The name of the venue"
        },
        "url": {
          "type": "string"
        }
      },
      "required": [
        "venueType",
        "venueId",
        "venueName"
      ]
    },
    "EventInstance": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "venueIds": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "description": "The IDs of the venues where this event instance takes place"
        },
        "start": {
          "type": "ref",
          "ref": "PartialDate",
          "description": "The start date and/or time"
        },
        "end": {
          "type": "ref",
          "ref": "PartialDate",
          "description": "The end date and/or time"
        },
        "status": {
          "type": "ref",
          "ref": "EventStatus",
          "description": "The status of the event instance"
        },
        "activities": {
          "type": "array",
          "items": {
            "type": "ref",
            "ref": "EventActivity"
          },
          "description": "The activities taking place during this event instance"
        }
      },
      "required": [
        "venueIds"
      ],
      "description": "A part of an event that can occur at a known or unknown date and/or time and a known or unknown place or places."
    },
    "PartialDate": {
      "type": "string",
      "description": "An ISO 8601 date and time string that may be incomplete (e.g. '2023', '2023-05') and does not include timezone information (forced UTC)"
    },
    "EventActivity": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "name": {
          "type": "ref",
          "ref": "Translations",
          "description": "The name of the activity"
        },
        "description": {
          "type": "ref",
          "ref": "Translations",
          "description": "The description of the activity"
        }
      },
      "required": [
        "name"
      ]
    },
    "EventComponent": {
      "type": "union",
      "refs": [
        "unknown"
      ]
    },
    "LinkComponent": {
      "type": "object",
      "properties": {
        "url": {
          "type": "string",
          "description": "The URL of the link"
        },
        "name": {
          "type": "ref",
          "ref": "Translations",
          "description": "The name of the link"
        },
        "description": {
          "type": "ref",
          "ref": "Translations",
          "description": "A description of the link"
        },
        "disabled": {
          "type": "boolean",
          "description": "Whether the link is disabled"
        },
        "opensAt": {
          "type": "ref",
          "ref": "PartialDate",
          "description": "The date and/or time when the link becomes active"
        },
        "closesAt": {
          "type": "ref",
          "ref": "PartialDate",
          "description": "The date and/or time when the link becomes inactive"
        }
      },
      "required": [
        "url"
      ]
    }
  }
}